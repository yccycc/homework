<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:gravity="center"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
>
        <LinearLayout android:layout_width="wrap_content"
                      android:id="@+id/chart_space_ll"
                      android:orientation="vertical"
                      android:layout_height="200dp">

        </LinearLayout>
        <LinearLayout android:layout_width="wrap_content"
                      android:gravity="center"
                      android:orientation="horizontal"
                      android:layout_height="wrap_content">
            <ImageView android:layout_width="10dp" android:layout_height="10dp"
                       android:background="@color/wait_done_color"/>
            <TextView android:layout_width="wrap_content"
                      android:textSize="10sp"
                      android:textColor="@color/wait_done_color"
                      android:text="@string/wait_done"
                      android:layout_height="wrap_content"/>
            <ImageView android:layout_width="10dp"
                       android:layout_marginLeft="20dp"
                       android:layout_height="10dp"
                       android:background="@color/wait_uploaded_color"/>
            <TextView android:layout_width="wrap_content"
                      android:textSize="10sp"
                      android:textColor="@color/wait_uploaded_color"
                      android:text="@string/wait_uploaded"
                      android:layout_height="wrap_content"/>
        </LinearLayout>
</LinearLayout>

